{
  "manifest_version": 2,
  "name": "Scout",
  "description":
    "This is an extension that replaces github's scary red x with the üôè emoji",
  "version": "0.0.1",
  "browser_action": {
    "default_icon": {
      "19": "images/icon-38.png",
      "38": "images/icon-38.png"
    }
  },
  "icons": {
    "38": "images/icon-38.png",
    "128": "images/icon-128.png"
  },
  "web_accessible_resources": ["images/please.png"],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["github-review-icon-replacer.js"]
    }
  ],
  "permissions": ["tabs"]
}
